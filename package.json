{
  "name": "gap-automated-tests",
  "version": "1.0.0",
  "description": "Automated test repo for Find a Grant service",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cy:open:watch": "npx cypress open --config watchForFileChanges=true",
    "cy:open:nowatch": "npx cypress open --config watchForFileChanges=false",
    "cy:open": "npx cypress open --config watchForFileChanges=false --e2e --browser chrome",
    "cy:run:all": "npx cypress run",
    "cy:run:find": "npx cypress run --spec \"cypress/e2e/find/*.cy.js\"",
    "cy:run:apply": "npx cypress run --spec \"cypress/e2e/applicant/*.cy.js\"",
    "cy:run:admin": "npx cypress run --spec \"cypress/e2e/admin/*.cy.js\"",
    "cy:run:superadmin": "npx cypress run --spec \"cypress/e2e/superadmin/*.cy.js\"",
    "cy:run:apidashboard": "npx cypress run --spec \"cypress/e2e/api-dashboard/*.cy.js\"",
    "lint": "npx eslint --ext .js,.ts ./",
    "lint:fix": "npx eslint --ext .js,.ts ./ --fix",
    "prettier:write": "npx prettier . --write",
    "prettier:check": "npx prettier . --check",
    "prepare": "husky install",
    "clean": "rm -rf node_modules package-lock.json",
    "clean:install": "npm run clean && npm install",
    "vpc:add": "source .env && aws ec2 authorize-security-group-ingress --group-id $AWS_VPC_SG_ID --protocol tcp --port 5432 --cidr $(curl ifconfig.me)/32",
    "vpc:remove": "source .env && aws ec2 revoke-security-group-ingress --group-id $AWS_VPC_SG_ID --protocol tcp --port 5432 --cidr $(curl ifconfig.me)/32",
    "vpc:add:print": "source .env && echo \"aws ec2 authorize-security-group-ingress --group-id $AWS_VPC_SG_ID --protocol tcp --port 5432 --cidr $(curl ipconfig.io/ip)/32\"",
    "env:switch": "node envSwitch.js",
    "subs": "node getSubs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cabinetoffice/gap-automated-tests.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cabinetoffice/gap-automated-tests/issues"
  },
  "homepage": "https://github.com/cabinetoffice/gap-automated-tests#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "cypress": "^13.2.0",
    "cypress-mochawesome-reporter": "^3.6.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "mochawesome": "^7.1.3",
    "prettier": "3.0.3",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "**/*.js": "eslint --ext .js,.ts ./ --fix",
    "**/*.ts": "eslint --ext .js,.ts ./ --fix"
  },
  "dependencies": {
    "@aws-sdk/client-api-gateway": "^3.490.0",
    "@percy/cypress": "^3.1.2",
    "contentful-management": "^10.46.2",
    "cypress-file-upload": "^5.0.8",
    "dayjs": "^1.11.10",
    "decompress": "^4.2.1",
    "dotenv": "^16.3.1",
    "fs": "^0.0.1-security",
    "node-xlsx": "^0.23.0",
    "pg": "^8.11.3"
  }
}
