{
  "name": "gap-automated-tests",
  "version": "1.0.0",
  "description": "Automated test repo for Find a Grant service",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cy:open:watch": "npx cypress open --config watchForFileChanges=true",
    "cy:open:nowatch": "npx cypress open --config watchForFileChanges=false",
    "cy:run": "npx cypress run",
    "lint": "npx eslint",
    "prettier:write": "npx prettier . --write",
    "prettier:check": "npx prettier . --check",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cabinetoffice/gap-automated-tests.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cabinetoffice/gap-automated-tests/issues"
  },
  "homepage": "https://github.com/cabinetoffice/gap-automated-tests#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "cypress": "^13.2.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "mochawesome": "^7.1.3",
    "prettier": "3.0.3",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@percy/cypress": "^3.1.2",
    "contentful-management": "^10.46.2",
    "dotenv": "^16.3.1",
    "fs": "^0.0.1-security",
    "pg": "^8.11.3"
  }
}
